{
  "name": "docs",
  "version": "1.0.92",
  "description": "Documentation for vue-facebook-component-login",
  "author": "Adi Sahar",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "yarn serve",
    "serve": "vue-cli-service serve --host localhost --https",
    "update": "yarn add vue-facebook-login-component",
    "build": "vue-cli-service build",
    "build:inspect": "vue-cli-service inspect > output.js",
    "lint": "vue-cli-service lint",
    "prerelease": "yarn && yarn update & yarn version --patch --no-git-tag-version && yarn build",
    "release": "node ./gh-pages",
    "postrelease": "node ./post-release"
  },
  "dependencies": {
    "@vue/composition-api": "^0.5.0",
    "animate.css": "^3.7.2",
    "bootstrap": "^4.4.1",
    "core-js": "^3.6.4",
    "lodash": "^4.17.19",
    "lodash.reject": "^4.6.0",
    "npm-check": "^5.9.2",
    "smoothscroll-polyfill": "^0.4.4",
    "vue": "^2.6.11",
    "vue-analytics": "^5.22.1",
    "vue-breakpoint-component": "^1.2.6",
    "vue-components": "^1.0.0",
    "vue-facebook-login-component": "^2.3.0",
    "vue-flatten-routes": "^1.1.0",
    "vue-github-button": "^1.1.2",
    "vue-markdown": "^2.2.4",
    "vue-router": "^3.1.6",
    "vue-use-web": "^0.10.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.2.3",
    "@vue/cli-plugin-eslint": "^4.2.3",
    "@vue/cli-service": "^4.2.3",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^6.2.2",
    "gh-pages": "^2.2.0",
    "html-loader": "^0.5.5",
    "markdown-loader": "^5.1.0",
    "sass": "^1.26.2",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "warn",
      "no-unused-vars": "warn"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
